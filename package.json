{
  "name": "tunic",
  "version": "0.3.0",
  "description": "The stupid doc-block parser.",
  "keywords": [
    "toga",
    "tunic",
    "parser",
    "docblock",
    "docs",
    "doc",
    "dox"
  ],
  "homepage": "https://github.com/togajs/tunic",
  "bugs": "https://github.com/togajs/tunic/issues",
  "license": "MIT",
  "author": "Shannon Moeller <me@shannonmoeller.com> (http://shannonmoeller.com)",
  "main": "dist/index.js",
  "repository": {
    "type": "git",
    "url": "https://github.com/togajs/tunic"
  },
  "dependencies": {
    "mtil": "^0.1.3"
  },
  "devDependencies": {
    "babel": "^5.1.13",
    "coveralls": "^2.11.2",
    "eslint": "^0.20.0",
    "expect": "^1.6.0",
    "isparta": "^3.0.3",
    "mocha": "^2.2.4",
    "nodemon": "^1.3.7",
    "stream-array": "^1.1.0",
    "vinyl-fs": "^1.0.0"
  },
  "engines": {
    "node": ">= 0.10"
  },
  "scripts": {
    "build": "babel index.js --out-file dist/index.js",
    "cover": "babel-node ./node_modules/.bin/isparta cover --report text --report lcov ./node_modules/.bin/_mocha",
    "coveralls": "cat ./coverage/lcov.info | coveralls",
    "lint": "eslint index.js test/*.js",
    "test": "npm run lint && npm run cover",
    "watch": "nodemon --exec 'npm run test' --watch index.js --watch test"
  }
}
